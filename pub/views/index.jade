div.container
  header#banner(role='banner')
    h3.main= title
    h3.info.user(title='click to sign out')
      a.title.logout(href='#')= titleInfo
  div#content
    div#nav
      ul.tabs
        li
          a#feeds-tab Feeds
        li
          a#posts-tab.active Posts
    div#feeds-content.tab-content
      div#feeds-edit
        input(name='feed-input', type='text', placeholder='Enter a feed name or url')
        input(value='subscribe', type='button')
      ul#feeds-list
    div#posts-content.tab-content
      ul#posts-list
