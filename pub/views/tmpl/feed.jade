script(id='feed-template', type='text/html')
  div.header
    div.unselectable.delete.iconic(title='unsubscribe') X
    div.unselectable.vis.item-vis.iconic(title='toggle feed visibility') y
    a.unselectable.feed-link.iconic(href='{{link}}', target='_blank', title='open feed in new window') 1
    div.text
      h4.title {{title}}
      h4.author {{authorText}}
      h5.unread-posts {{unreadPosts}}
  h5.description {{{description}}}

