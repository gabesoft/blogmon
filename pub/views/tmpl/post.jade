script(id='post-template', type='text/html')
  div.post-content.collapsed
    div.header
      div.header-inner
        h5.left {{feedTitle}}
        span.right
          h5.date {{dateStr}}
          a.iconic(href='{{link}}', target='_blank', title='open blog in new window') 1
        h4 {{title}}
    div.description {{{description}}}

